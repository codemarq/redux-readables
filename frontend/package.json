    "redux-mock-store": "1.4.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codemarq/redux-readables"
  }
