{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Redux-Readables App using react, redux, npmscripts, enzyme, mocha, expect",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "build": "mkdir -p dist",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "John-Mike Marquardt",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bootstrap": "4.0.0",
    "jquery": "^3.3.1",
    "moment": "^2.20.1",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "16.2.0",
    "react-loading": "^1.0.3",
    "react-redux": "5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "^6.26.0",
    "cheerio": "1.0.0-rc.2",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "enzyme": "3.3.0",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "0.9.6",
    "expect": "22.1.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.5",
    "jsdom": "^11.5.0",
    "mocha": "^5.0.0",
    "nock": "^9.0.0",
